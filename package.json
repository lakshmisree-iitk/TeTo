{
  "name": "teto-learning-platform",
  "version": "1.0.0",
  "description": "An online learning platform with video streaming, reading materials, and AI chatbot",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "server:python": "python start_server.py",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "start:python": "python start_server.py"
  },
  "keywords": ["learning", "education", "video", "chatbot", "ai"],
  "author": "TeTo Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "fs-extra": "^11.1.1",
    "@google/generative-ai": "^0.2.1",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
} 